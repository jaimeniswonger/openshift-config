[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=centos
ansible_become=true

deployment_type=origin

openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]

#containerized=true
openshift_release=v3.6.1
#openshift_image_tag=v1.5.1

openshift_public_hostname=openshift.keyhole-reference.info
openshift_master_default_subdomain=apps.keyhole-reference.info

openshift_master_overwrite_named_certificates=true

#openshift_hosted_logging_deploy=false
#openshift_logging_kibana_hostname=kibana.microservice.institute

#openshift_hosted_metrics_deploy=false
#openshift_metrics_hawkular_hostname=metrics.microservice.institute

[masters]
openshift.keyhole-reference.info openshift_schedulable=true

[nodes]
openshift.keyhole-reference.info openshift_node_labels="{'region': 'infra', 'zone': 'default'}"
#node1.microservice.institute openshift_node_labels="{'region': 'primary', 'zone': 'default'}"
#node2.microservice.institute openshift_node_labels="{'region': 'primary', 'zone': 'default'}"
